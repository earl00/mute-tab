{
    "manifest_version": 3,
    "name": "Tab Muter",
    "version": "1.0",
    "description": "A simple extension to mute and unmute tabs.",
    "icons": { "48": "icon.png" },
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
      "tabs",
      "activeTab"
    ],
    "browser_action": {
      "default_icon": "icon.png"
    }
  }
  